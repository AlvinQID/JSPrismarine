{
    "entryPoints": ["packages/*"],
    "tsconfig": "./tsconfig.json",
    "readme": "./README.md",
    "out": "./docs",
    "entryPointStrategy": "packages",
    "externalPattern": [
        "**/node_modules/**",
        ".*packages(/[^/]+)/.*"
    ],
    "excludeExternals": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "excludeInternal": true
}
